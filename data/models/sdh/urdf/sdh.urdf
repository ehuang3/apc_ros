		<joint name="finger_21_joint" type="revolute">
			<mimic joint="knuckle_joint" multiplier="1" offset="0"/>
			<origin xyz="-0.033 -0.01905 0.0745" rpy="0 0 1.5708" />
			<parent link="palm_link"/>
			<child link="finger_21_link"/>
