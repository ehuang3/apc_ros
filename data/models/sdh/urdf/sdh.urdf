			<parent link="palm_link"/>
			<child link="finger_11_link"/>
			<axis xyz="0 0 1" />
			<dynamics damping="1" />
			<limit effort="100" lower="0" upper="1.570796327" velocity="5"/>
		</joint>

		<link name="finger_11_link">
